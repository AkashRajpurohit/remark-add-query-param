{
  "name": "remark-add-query-param",
  "version": "2.0.0",
  "description": "A remark plugin to add query parameters to links",
  "repository": {
    "type": "git",
    "url": "https://github.com/AkashRajpurohit/remark-add-query-param.git"
  },
  "homepage": "https://github.com/AkashRajpurohit/remark-add-query-param",
  "bugs": "https://github.com/AkashRajpurohit/remark-add-query-param/issues",
  "author": {
    "name": "AkashRajpurohit",
    "email": "me@akashrajpurohit.com",
    "url": "https://akashrajpurohit.com"
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "query-params",
    "query-parameters",
    "query-string"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "tsup --watch",
    "format": "biome check --write ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "changeset": "changeset"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.3.3",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "remark": "^15.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}